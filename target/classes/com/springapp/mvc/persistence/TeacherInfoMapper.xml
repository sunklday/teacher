<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.springapp.mvc.persistence.TeacherInfoMapper">
    <select id="getTeacherInfo" resultType="com.springapp.mvc.domain.TeacherInfo">
        SELECT * FROM TEACHERINFO
        WHERE TEACHERINFO.name = #{name}
    </select>
</mapper>